                                && !(/Mobile/.test(navigator.userAgent))) {

                                var currentPageURL = window.location.href;
                                var currentPageRelativeURL = currentPageURL.replace(/^(?:\/\/|[^\/]+)*\//, "/");

                                var redirectURL = currentPageRelativeURL;
