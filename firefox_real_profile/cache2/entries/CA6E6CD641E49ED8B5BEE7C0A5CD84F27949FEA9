//STARTADORSTATIC
const startAdOrStatic = () => {
    const staticImage = document.getElementById('staticImage');
    const staticImageSrcPath = staticImage.firstChild.getAttribute('src');

    staticImageSrcPath.includes('ComcastBlankStaticPlaceholderImage.png')
        ? startAd()
        : (myFT.applyClickTag(myFT.$('#staticImage'), 1, myFT.instantAds.clickTag_URL),
            staticImage.style.display = 'block');
};

//LOAD DYNAMIC TEXT VARIABLES
let elementId = "timelines";
let inputElement = document.getElementById(elementId);

const elementIds = ["timelines","F01Headline01","F01Headline02","F01LegalDisclaimer","F02Headline01","F02Headline02", "F02LegalDisclaimer","F03Headline01","F03Headline02","F03LegalDisclaimer","F04Headline01","F04Headline02","F04LegalDisclaimer","FFTHeadline01","FFTHeadline02","FFTLegalDisclaimer","FFPHeader","FFPHeadline","FFPPrice","FFPBoltOn","FFPLegalDisclaimer","FFCHeader","FFCHeadline","FFCPriceCard","FFCBoltOn","FFCLegalDisclaimer","CTAText","PhoneNumber"];

elementIds.forEach((elementId) => {
  const inputElement = document.getElementById(elementId);
  inputElement.innerHTML = myFT.instantAds[elementId];
});

//COLORS PATH VARIABLES 
document.getElementById("CTAContainer").style.backgroundColor = myFT.instantAds.CTAPathColor;

//STARTAD
function startAd() {
    
    let tlMain = gsap.timeline();
    
//LOAD DYNAMIC IMAGES VARIABLES
    function loadImages(id){
    let images = {};
    let div = document.getElementById(id);
    images[id] = document.createElement('img');
    images[id].src = myFT.instantAds[id];
    div.appendChild(images[id]);
    } 
    
    ['Background01','Background02','CBLogo','F01Image','F02Image','F03Image','F04Image','FFCCard','LightGlowImage01','LightPathImage01'].forEach(loadImages);
        
//SET FONT SIZE DISCLAIMERS
    function disclaimers() {
    const disclaimerIDs = ['F01LegalDisclaimer','F02LegalDisclaimer','F03LegalDisclaimer','F04LegalDisclaimer','FFTLegalDisclaimer', 'FFPLegalDisclaimer','FFCLegalDisclaimer'];

Â  Â  let shouldChangeFontSize = false;

Â  Â  disclaimerIDs.forEach((id) => {

Â  Â  const disclaimer = document.getElementById(id);

Â  Â  const lines = disclaimer.innerHTML.split('<br>').length;

Â  Â  shouldChangeFontSize = lines >= 8 ? true : shouldChangeFontSize;

Â  Â  });

Â  Â  disclaimerIDs.forEach((id) => {

Â  Â  Â  Â  const disclaimer = document.getElementById(id);

Â  Â  Â  Â  disclaimer.style.fontSize = shouldChangeFontSize ? '7px' : '8px';

Â  Â      });
    }

//SET STARTING POSITIONS
    (function init() {
        gsap.set([LightImageContainer01, LightImageContainer02],{height:0, autoAlpha:0})
        
        gsap.set([LightGlowImage01, LightGlowImage02],{autoAlpha:0})
        
        gsap.set([CBLogoContainer, F01HeadlineHolder, F02HeadlineHolder, F03HeadlineHolder, F04HeadlineHolder, CTAContainer, PhoneNumberContainer], {y:'-=10', autoAlpha:0})
        
        gsap.set([BackgroundContainer02, F01LegalDisclaimerHolder, F02LegalDisclaimerHolder, F03LegalDisclaimerHolder, F04LegalDisclaimerHolder, FFTLegalDisclaimerHolder, FFPLegalDisclaimerHolder, FFCLegalDisclaimerHolder], {autoAlpha:0})
        
        gsap.set(F01ImageContainer, {x:'+=160'})
        
        gsap.set([F02ImageContainer, F03ImageContainer, F04ImageContainer], {autoAlpha:0})
        
        gsap.set([FFTHeadlineHolder, FFPOfferHolder, FFCOfferHolder],{x:'-=14', autoAlpha:0})
                
                        
        animation();
        disclaimers();
        
	})();
    
//ANIMATION FRAME01
    function frame01() {
        var tl = gsap.timeline();

        tl
        .to(CBLogoContainer, {duration:.6,  y:'+=10', autoAlpha:1, ease: "sine.out"})
        
        .to(F01HeadlineHolder, {duration:.6, y:'+=10', autoAlpha:1, ease: "sine.out"}, "-=.3")
        .to(F01ImageContainer, {duration:.6, x:'-=160', autoAlpha:1, ease: "sine.out", onComplete: LightAnimation01}, "-=.6")
        
        .to([CTAContainer, PhoneNumberContainer], {duration:.6, y:'+=10', autoAlpha:1, ease: "sine.out"}, "-=.3")
        
        .to(F01LegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
    
        return tl;
    };
        
//ANIMATION FRAME02
    function frame02() {
        var tl = gsap.timeline();

        tl 
        .to(F01HeadlineHolder, {duration:.4, y:'+=10', autoAlpha:0, ease: "sine.out", onStart: imageTransitionAnimation01})
        .to(F01LegalDisclaimerHolder, {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
            
        .to(F02HeadlineHolder, {duration:.6, y:'+=10', autoAlpha:1, ease: "sine.out"})
                
        .to(F02LegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
    
        return tl;
    };
    
//ANIMATION FRAME03
    function frame03() {
        var tl = gsap.timeline();

        tl 
        .to(F02HeadlineHolder, {duration:.4, y:'+=10', autoAlpha:0, ease: "sine.out", onStart: imageTransitionAnimation02})
        .to(F02LegalDisclaimerHolder, {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
            
        .to(F03HeadlineHolder, {duration:.6, y:'+=10', autoAlpha:1, ease: "sine.out"})
                
        .to(F03LegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
            
        return tl;
    };
    
//ANIMATION FRAME04
    function frame04() {
        var tl = gsap.timeline();

        tl 
        .to(F03HeadlineHolder, {duration:.4, y:'+=10', autoAlpha:0, ease: "sine.out", onStart: imageTransitionAnimation03})
        .to(F03LegalDisclaimerHolder, {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
            
        .to(F04HeadlineHolder, {duration:.6, y:'+=10', autoAlpha:1, ease: "sine.out"})
                
        .to(F04LegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
            
        return tl;
    };
    
//ANIMATION ENDFRAMETEXT
    function endFrameText() {
        var tl = gsap.timeline();

        tl
        .to(BackgroundContainer02, {duration:.4, autoAlpha:1, ease: "sine.out"})
        .to([CTAContainer, PhoneNumberContainer], {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
        
        .set([CTAContainer, PhoneNumberContainer],{x:'-=14'})
        
        .to(FFTHeadlineHolder, {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out"})
        
        .to([CTAContainer, PhoneNumberContainer], {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out"}, "-=.3")
        
        .to(FFTLegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
            
        return tl;
    };
    
//ANIMATION ENDFRAMEPRICE
    function endFramePrice() {
        var tl = gsap.timeline();

        tl
        .to(BackgroundContainer02, {duration:.4, autoAlpha:1, ease: "sine.out"})
        .to([CTAContainer, PhoneNumberContainer], {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
        
        .set([CTAContainer, PhoneNumberContainer],{x:'-=14'})
        
        .to(FFPOfferHolder, {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out"})
        
        .to([CTAContainer, PhoneNumberContainer], {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out"}, "-=.3")
        
        .to(FFPLegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
            
        return tl;
    };
    
//ANIMATION ENDFRAMECARD
    function endFrameCard() {
        var tl = gsap.timeline();

        tl
        .to(BackgroundContainer02, {duration:.4, autoAlpha:1, ease: "sine.out"})
        .to([CTAContainer, PhoneNumberContainer], {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
        
        .set([CTAContainer, PhoneNumberContainer],{x:'-=14'})
        
        .to(FFCOfferHolder, {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out", onComplete: LightAnimation02})
        
        .to([CTAContainer, PhoneNumberContainer], {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out"}, "-=.3")
        
        .to(FFCLegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
            
        return tl;
    };
    
//ANIMATION ENDFRAMEPRICEALT
    function endFramePriceAlt() {
        var tl = gsap.timeline();

        tl
        .to(FFCOfferHolder, {duration:.4, y:'+=10', autoAlpha:0, ease: "sine.out"})
        .to([FFCLegalDisclaimerHolder, LightImageContainer02], {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
        
        .to(FFPOfferHolder, {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out"})
        
        .to(FFPLegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
            
        return tl;
    };
    
//ANIMATION ENDFRAMECARDALT
    function endFrameCardAlt() {
        var tl = gsap.timeline();

        tl
        .to(FFPOfferHolder, {duration:.4, y:'+=10', autoAlpha:0, ease: "sine.out"})
        .to(FFPLegalDisclaimerHolder, {duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.4")
        
        .to(FFCOfferHolder, {duration:.6, x:'+=14', autoAlpha:1, ease: "sine.out", onComplete: LightAnimation02})
        
        .to(FFCLegalDisclaimerHolder, {duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.3")
                    
        return tl;
    };

            
//ANIMATION TIMELINES
    function mainTimeline() {
        return tlMain
            .set(container, { visibility: 'visible' })
            .add(frame01(), "+=0")
    }
    
    //SET ANIMATION TIMELINES
    function animation() {
        let timelines = parseInt(inputElement.innerHTML);
        let totalTime = mainTimeline();
    
        switch (timelines) { 
            case 1:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(frame04(),"+=2").add(endFrameText(),"+=2");
                break;
            case 2:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(frame04(),"+=2").add(endFramePrice(),"+=2");
                break;
            case 3:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(frame04(),"+=2").add(endFrameCard(),"+=2");
                break;
            case 4:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(endFrameCard(),"+=2").add(endFramePriceAlt(),"+=2");
                break;
            case 5:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(endFramePrice(),"+=2").add(endFrameCardAlt(),"+=2");
                break;
            case 6:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(endFrameText(),"+=2");
                break;
            case 7:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(endFramePrice(),"+=2");
                break;
            case 8:
                totalTime.add(frame02(),"+=2").add(frame03(),"+=2").add(endFrameCard(),"+=2");
                break;
            case 9:
                totalTime.add(frame02(),"+=2").add(endFrameCard(),"+=2").add(endFramePriceAlt(),"+=2");
                break;
            case 10:
                totalTime.add(frame02(),"+=2").add(endFramePrice(),"+=2").add(endFrameCardAlt(),"+=2");
                break;
            case 11:
                totalTime.add(frame02(),"+=2").add(endFrameText(),"+=2");
                break;
            case 12:
                totalTime.add(frame02(),"+=2").add(endFramePrice(),"+=2");
                break;
            case 13:
                totalTime.add(frame02(),"+=2").add(endFrameCard(),"+=2");
                break;
            case 14:
                totalTime.add(endFrameCard(),"+=2").add(endFramePriceAlt(),"+=2");
                break;
            case 15:
                totalTime.add(endFramePrice(),"+=2").add(endFrameCardAlt(),"+=2");
                break;
            case 16:
                totalTime.add(endFrameText(),"+=2");
                break;
            case 17:
                totalTime.add(endFramePrice(),"+=2");
                break;
            case 18:
                totalTime.add(endFrameCard(),"+=2");
                break;
            default:
                console.log("TimelineError");
                break;
        }
        let totalDuration = totalTime.totalDuration();
        console.log("totalDuration: " , totalDuration);
    }
    
//SECONDARY ANIMATIONS
    
    function LightAnimation01(){
    
        var tl = gsap.timeline();
    
        tl
        .to(LightImageContainer01,{duration:1, height:261, autoAlpha:1, ease: "expo.out"})
        
        .to(LightGlowImage01,{duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.8")
        
        console.log('LightAnimation01');
    }
    
    function LightAnimation02(){
    
        var tl = gsap.timeline();
    
        tl
        .to(LightImageContainer02,{duration:1, height:250, autoAlpha:1, ease: "expo.out"})
        
        .to(LightGlowImage02,{duration:.6, autoAlpha:1, ease: "sine.out"}, "-=.8")
        
        console.log('LightAnimation02');
    }
    
    function imageTransitionAnimation01(){
    
        var tl = gsap.timeline();
    
        tl
        .to(F02ImageContainer,{duration:.6, autoAlpha:1, ease: "sine.out"})
        
        .to(F01ImageContainer,{duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.2")
        
        console.log('imageTransitionAnimation01');
    }
    
    function imageTransitionAnimation02(){
    
        var tl = gsap.timeline();
    
        tl
        .to(F03ImageContainer,{duration:.6, autoAlpha:1, ease: "sine.out"})
        
        .to(F02ImageContainer,{duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.2")
        
        console.log('imageTransitionAnimation02');
    }
    
    function imageTransitionAnimation03(){
    
        var tl = gsap.timeline();
    
        tl
        .to(F04ImageContainer,{duration:.6, autoAlpha:1, ease: "sine.out"})
        
        .to(F03ImageContainer,{duration:.4, autoAlpha:0, ease: "sine.out"}, "-=.2")
        
        console.log('imageTransitionAnimation03');
    }

    

};
